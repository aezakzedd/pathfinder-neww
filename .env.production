# Production Environment Configuration for Raspberry Pi

# Disable development tools
VITE_ENABLE_DEVTOOLS=false

# Set log level to errors only
VITE_LOG_LEVEL=error

# MapTiler API Key (replace with your key)
VITE_MAPTILER_API_KEY=your_maptiler_api_key_here

# Performance mode
VITE_PERFORMANCE_MODE=true

# Cache settings
VITE_ENABLE_SW_CACHE=true
